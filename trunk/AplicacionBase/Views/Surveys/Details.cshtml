@model AplicacionBase.Models.Survey

@{
    ViewBag.Title = "Details";
}
<div class="c9">
<h2>Detalles</h2>

<fieldset>
    <div class="display-label"><h3>Nombre</h3></div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>
    <br/>

    <div class="display-label"><h3>Objetivo</h3></div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Aim)
    </div>
    <br/>
</fieldset>
<br/>
    <p>
    @Html.ActionLink("Editar", "Edit", new { id=Model.Id }, htmlAttributes: new {@class = "ebutton blue"}) |
    @Html.ActionLink("Cancelar", "Index", null ,htmlAttributes: new { @class = "ebutton blue" })
</p>
</div>
