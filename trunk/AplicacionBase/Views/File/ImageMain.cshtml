@model IEnumerable<AplicacionBase.Models.File>

@{
    Layout = ""; 
}
@if (Model.Count() == 0)
{
    <img src="../../Content/themes/base/images/citacion_prueba.jpg" />
}
else
{
    foreach (var item in Model)
    {    
       <img alt="" src= @Html.DisplayFor(modelItem => item.Path)/>
    }
}