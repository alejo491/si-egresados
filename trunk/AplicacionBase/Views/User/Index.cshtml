@model IEnumerable<AplicacionBase.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Usuarios Registrados</h2>

<p>
    @Html.ActionLink("Registrar Nuevo Usuario", "Create")
</p>
<table>
    <tr>
        <th>
            Nombres
        </th>
        <th>
            Apellidos
        </th>
        <th>
            Direccion
        </th>
        <th>
            Celular
        </th>
         <th>
            Telefono
        </th>
        <th>
            Opciones
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstNames)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastNames)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CellphoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Ver", "Details", new { id = item.Id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
